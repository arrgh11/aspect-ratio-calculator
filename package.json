{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "aspect-ratio-calculator",
  "title": "Aspect Ratio Calculator",
  "description": "Give it dimensions and it will calculate the aspect ratio for you",
  "icon": "command-icon.png",
  "author": "sam.miller",
  "owner": "creative2",
  "categories": [
    "Developer Tools",
    "Design Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Aspect Ratio",
      "description": "calculates aspect ratios",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}